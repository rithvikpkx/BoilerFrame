{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3AccessForBoilerFrame",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::my-boilerframe-boilerframe-uploads",
        "arn:aws:s3:::my-boilerframe-boilerframe-uploads/*"
      ]
    },
    {
      "Sid": "RekognitionOperations",
      "Effect": "Allow",
      "Action": [
        "rekognition:CreateCollection",
        "rekognition:DeleteCollection",
        "rekognition:IndexFaces",
        "rekognition:StartFaceSearch",
        "rekognition:GetFaceSearch",
        "rekognition:ListCollections",
        "rekognition:DescribeCollection"
      ],
      "Resource": "*"
    }
  ]
}
